"Shared.Guide.Heading.TonesInput" = "Tones Input";
"Shared.Guide.Body.TonesInput" = "v = 1, High Level (陰平)\nx = 2, Medium Rising (陰上)\nq = 3, Medium Level (陰去)\nvv = 4, Low Falling (陽平)\nxx = 5, Low Rising (陽上)\nqq = 6, Low Level (陽去)";
"Shared.Guide.Example.TonesInput" = "Examples: sinq = sin3(線), sinqq = sin6(羨)";

"Shared.Guide.Heading.CangjieReverseLookup" = "Lookup Jyutping with Cangjie";
"Shared.Guide.Body.CangjieReverseLookup" = "Start with “v”, then enter Cangjie5 code. For example, enter “vdam”, candidate “查” will appear. Candidates will be displayed with the corresponding Jyutping.";

"Shared.Guide.Heading.PinyinReverseLookup" = "Lookup Jyutping with Pinyin";
"Shared.Guide.Body.PinyinReverseLookup" = "Start with “r”, then enter Pinyin code. For example, enter “rcha”, candidate “查” will appear. Candidates will be displayed with the corresponding Jyutping.";

"Shared.Guide.Heading.StrokeReverseLookup" = "Lookup Jyutping with Stroke";
"Shared.Guide.Body.StrokeReverseLookup" = "Start with “x”, then enter Stroke code. For example, enter “xwsad”, candidate “木” will appear. Candidates will be displayed with the corresponding Jyutping.";
"Shared.Guide.Example.StrokeReverseLookup" = "w = 橫(waang), Horizontal\ns = 豎(syu), Vertical\na = 撇, Left-falling\nd = 點(dim), Right-falling\nz = 折(zit), Turning";

"Shared.Guide.Heading.ComposeReverseLookup" = "Lookup Jyutping with Loengfan";
"Shared.Guide.Body.ComposeReverseLookup" = "Start with “q”, then enter two components' Jyutping. For example, enter “qmukdaan”, candidate “查” will appear. Candidates will be displayed with the corresponding Jyutping.";

